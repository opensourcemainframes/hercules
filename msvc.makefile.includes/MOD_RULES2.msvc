!IFNDEF NOCRYPTO
crypto: $(X)dyncrypt.dll

$(X)dyncrypt.dll: $(dyncrypt_OBJ) $(O)hengine.lib $(O)hutil.lib $(O)hsys.lib $(O)hercver.res
    $(linkdll)

